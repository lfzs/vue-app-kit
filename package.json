{
  "name": "vue-app-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "yarn && cross-env APP_ENV=development BASE_URL=/ webpack-dev-server",
    "dev": "yarn && cross-env APP_ENV=development BASE_URL=/ webpack",
    "staging": "yarn && cross-env APP_ENV=staging BASE_URL=/ webpack",
    "build": "yarn && cross-env APP_ENV=production BASE_URL=/ webpack",
    "lint": "yarn && stylelint --max-warnings 0 \"src/**/*.{vue,html,css,less,scss,sass}\" && eslint --max-warnings 0 \"src/**/*.{js,vue}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    },
    "commit-msg": "echo $HUSKY_GIT_PARAMS"
  },
  "lint-staged": {
    "src/**/*.{vue,html,css,less,scss,sass}": "stylelint --max-warnings 0",
    "src/**/*.{js,vue}": "eslint --max-warnings 0",
    "./*.js": "eslint --max-warnings 0"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.12.5",
    "@capacitor/android": "2.4.6",
    "@capacitor/cli": "^2.4.6",
    "@capacitor/core": "^2.4.6",
    "@capacitor/ios": "2.4.6",
    "@ionic/vue": "^5.5.3",
    "@ionic/vue-router": "^5.5.3",
    "axios": "^0.21.1",
    "dayjs": "^1.10.4",
    "es6-promise": "^4.2.8",
    "lodash": "^4.17.20",
    "normalize.css": "^8.0.1",
    "vue": "^3.0.5",
    "vue-router": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-proposal-private-methods": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@vue/compiler-sfc": "^3.0.5",
    "autoprefixer": "^10.2.4",
    "babel-loader": "^8.2.2",
    "babel-plugin-lodash": "^3.3.4",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.4.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "eslint": "^7.19.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-vue": "^7.5.0",
    "eslint-webpack-plugin": "^2.4.3",
    "file-loader": "^6.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^4.5.1",
    "husky": "^4.3.8",
    "less": "^4.1.1",
    "less-loader": "^7.3.0",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^1.3.5",
    "postcss": "^8.2.4",
    "postcss-loader": "^4.1.0",
    "sass": "^1.32.6",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.9.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "vue-eslint-parser": "^7.4.1",
    "vue-loader": "^16.1.2",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
